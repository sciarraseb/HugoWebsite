add_line_numbers <- function(before, options, envir) {
if (!is.null(before)) {
lines <- unlist(strsplit(before, "\n"))
n <- length(lines)
res <- paste0("<pre>", paste0("<span class='line-number'>", 1:n, "</span>", lines), "</pre>")
return(res)
}
}
